{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-minimize",
    "core:window:allow-close",
    "websocket:default",
    "websocket:default",
    "shell:default",
    "shell:allow-open",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "DuoBoloClient",
          "args": [
            "-i",
            {
              "validator": "\\S+"
            }
          ],
          "sidecar": true
        }
      ]
    }
  ]
}